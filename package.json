{
  "name": "@mobizerg/nest-logger",
  "description": "Winston logger integration module for nestjs framework",
  "version": "0.1.2",
  "author": "Mobizerg",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "nestjs",
    "logger",
    "winston",
    "typescript",
    "mobizerg"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
