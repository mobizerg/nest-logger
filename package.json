    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "winston": "^3.3.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/mobizerg/nest-logger",
  "repository": {
    "type": "git",
    "url": "git://github.com/mobizerg/nest-logger.git"
